{
  "name": "pyvet-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "dev": "concurrently \"json-server --watch back-end/db.json\"  \"ng serve --host 0.0.0.0 --configuration development -o\" ",
    "build": "ng build --output-path dist/pyvet/",
    "b:watch": "ng build --watch --configuration development",
    "test": "ng test",
    "create-lib": "ng generate library",
    "build-pack-lib": "ng build $PROJECTNAME && npm run pack-lib",
    "pack-lib": "cd dist/$PROJECTNAME && npm pack"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "rxjs": "~7.5.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.11",
    "@angular/cli": "~13.3.11",
    "@angular/compiler-cli": "~13.3.0",
    "@types/jasmine": "~3.10.0",
    "@types/node": "^12.11.1",
    "concurrently": "^8.0.1",
    "http-proxy-middleware": "^2.0.6",
    "jasmine-core": "~4.0.0",
    "json-server": "^0.17.3",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "~4.6.2"
  }
}
